    <info
        organisation="csipiemonte"
        module="«getClientLibArtifactName(this, model)»"
        revision="«this.versione».0" />
    <publications>
        <artifact name="«getClientLibArtifactNameWithVersion(this, model)»" type="jar" />   
    </publications>
</ivy-module>
«ENDFILE»
