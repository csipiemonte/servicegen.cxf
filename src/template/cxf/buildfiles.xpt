«DEFINE wsSecurityPropertiesFile(SOABEModel model) FOR ServiceDef»
«IF !this.bindings.isEmpty-»
	«LET (WSBinding)this.bindings.get(0) AS wsBinding-»	
		«FOREACH wsBinding.getAllEndPointWS() AS endpoint-»
		«LET wsSecurityPropertiesPath(model, this)+ this.codServizio+getCodeEndpoint(endpoint).toFirstUpper() AS pathTmp-»
			«FILE pathTmp+".ks.properties"»
			«EXPAND ksPropertiesFile(this.codServizio+getCodeEndpoint(endpoint).toFirstUpper(),endpoint) FOR this-»
			«ENDFILE»
			«FILE pathTmp+".ts.properties"»
			«EXPAND tsPropertiesFile(this.codServizio+getCodeEndpoint(endpoint).toFirstUpper(),endpoint) FOR this-»
			«ENDFILE»
			«FILE pathTmp+".encrypt.properties"»
			«EXPAND encryptPropertiesFile(this.codServizio+getCodeEndpoint(endpoint).toFirstUpper(),endpoint) FOR this-»
			«ENDFILE»
			«FILE pathTmp+".decrypt.properties"»
			«EXPAND decryptPropertiesFile(this.codServizio+getCodeEndpoint(endpoint).toFirstUpper(),endpoint) FOR this-»
			«ENDFILE»
		«ENDLET-»
		«ENDFOREACH-»
	«ENDLET-»
«ENDIF-»
«ENDDEFINE»

«DEFINE wsSecurityClientPropertiesFile(SOABEModel model, ServiceDef context) FOR RPCWebServiceConnector»
«LET wsSecurityPropertiesPath(model, context)+this.code AS pathTmp-»
	«FILE pathTmp+".ks.properties"»
	«EXPAND ksPropertiesFile(this.code, this.endpoint) FOR this.serviceDef-»
	«ENDFILE»
	«FILE pathTmp+".ts.properties"»
	«EXPAND tsPropertiesFile(this.code, this.endpoint) FOR this.serviceDef-»
	«ENDFILE»
	«FILE pathTmp+".encrypt.properties"»
	«EXPAND encryptPropertiesFile(this.code, this.endpoint) FOR this.serviceDef-»
	«ENDFILE»
	«FILE pathTmp+".decrypt.properties"»
	«EXPAND decryptPropertiesFile(this.code, this.endpoint) FOR this.serviceDef-»
	«ENDFILE»
«ENDLET»
«ENDDEFINE»

«DEFINE ksPropertiesFile(String prefix, WSEndpoint endpoint) FOR ServiceDef»
org.apache.ws.security.crypto.provider=org.apache.ws.security.components.crypto.Merlin
org.apache.ws.security.crypto.merlin.keystore.type=jks
org.apache.ws.security.crypto.merlin.keystore.password=@«prefix».ks.password.«getCodeEndpoint(endpoint)»@
org.apache.ws.security.crypto.merlin.file=@«prefix».ks.file.«getCodeEndpoint(endpoint)»@
«ENDDEFINE»

«DEFINE tsPropertiesFile(String prefix, WSEndpoint endpoint) FOR ServiceDef»
org.apache.ws.security.crypto.provider=org.apache.ws.security.components.crypto.Merlin
org.apache.ws.security.crypto.merlin.keystore.type=jks
org.apache.ws.security.crypto.merlin.keystore.password=@«prefix».ts.password.«getCodeEndpoint(endpoint)»@
org.apache.ws.security.crypto.merlin.file=@«prefix».ts.file.«getCodeEndpoint(endpoint)»@
«ENDDEFINE»

«DEFINE encryptPropertiesFile(String prefix, WSEndpoint endpoint) FOR ServiceDef»
org.apache.ws.security.crypto.provider=org.apache.ws.security.components.crypto.Merlin
org.apache.ws.security.crypto.merlin.keystore.type=jks
org.apache.ws.security.crypto.merlin.keystore.password=@«prefix».encrypt.password.«getCodeEndpoint(endpoint)»@
org.apache.ws.security.crypto.merlin.file=@«prefix».encrypt.file.«getCodeEndpoint(endpoint)»@
«ENDDEFINE»

«DEFINE decryptPropertiesFile(String prefix, WSEndpoint endpoint) FOR ServiceDef»
org.apache.ws.security.crypto.provider=org.apache.ws.security.components.crypto.Merlin
org.apache.ws.security.crypto.merlin.keystore.type=jks
org.apache.ws.security.crypto.merlin.keystore.password=@«prefix».decrypt.password.«getCodeEndpoint(endpoint)»@
org.apache.ws.security.crypto.merlin.file=@«prefix».decrypt.file.«getCodeEndpoint(endpoint)»@
«ENDDEFINE»

